{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "relative",
  "files.exclude": {
    "**/.git": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "{build,dist}/": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fddb40",
    "activityBar.activeBorder": "#029e82",
    "activityBar.background": "#fddb40",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#029e82",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#fdd10d",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#d5ae02",
    "titleBar.activeBackground": "#fdd10d",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#fdd10d99",
    "titleBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#fddb40",
    "statusBarItem.remoteBackground": "#fdd10d",
    "statusBarItem.remoteForeground": "#15202b"
  },
  "peacock.color": "fdd10d"
}
